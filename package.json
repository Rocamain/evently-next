{
  "name": "evently-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "lint:fix": "eslint . --fix --ext .js",
    "style:lint": "eslint '**/*.js'",
    "style:prettier": "prettier --check '**/*.{js,ts,tsx}'",
    "style:all": "npm run style:lint && npm run style:prettier",
    "pretty:fix": "prettier '**/*.{js,ts,tsx}' --write",
    "fix": "npx eslint \"./**/*.js\" --cache --cache-strategy content --fix"
  },
  "dependencies": {
    "aws-jwt-verify": "^4.0.0",
    "axios": "^1.4.0",
    "form-data": "^4.0.0",
    "next": "13.5.4",
    "react": "^18",
    "react-dom": "^18",
    "react-dropzone": "^14.2.3",
    "react-google-autocomplete": "^2.7.3",
    "sharp": "^0.32.5"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@types/google.maps": "^3.54.4",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "autoprefixer": "^10",
    "eslint": "^8",
    "eslint-config-next": "13.5.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "prettier": "^3.0.1",
    "tailwindcss": "^3",
    "typescript": "^5"
  }
}
